<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Comprobate" table="comprobate">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idCierreCaja" type="bigint" column="id_cierre_caja" nullable="false"/>
    <field name="nombreMozo" type="string" column="nombre_mozo" length="200" nullable="false"/>
    <field name="fecha" type="date" column="fecha" nullable="false"/>
    <field name="total" type="float" column="total" nullable="false"/>
  </entity>
</doctrine-mapping>
