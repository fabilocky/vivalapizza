<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Persona" table="persona">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dni" type="integer" column="dni" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="200" nullable="false"/>
    <field name="apellido" type="string" column="apellido" length="200" nullable="false"/>
    <field name="tipo" type="string" column="tipo" length="100" nullable="false"/>
  </entity>
</doctrine-mapping>
